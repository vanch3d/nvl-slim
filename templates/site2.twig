<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge"><![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1">
	{% block injectMETA %}
	{% endblock %}
    
    <title>{% block urltile %}Nicolas Van Labeke Website{% endblock %}</title>
    
    <link rel="shortcut icon" href="/assets/images/favicon.png" />
    <link rel="unapi-server" type="application/xml" title="unAPI" href="{{ app_base~'/api/unapi' }}" />

    <!-- Bootstrap -->
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
	<!-- Optional theme 
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap-theme.min.css"> -->
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css">
 	<link rel="stylesheet" media="screen" href="/plugins/elusive-iconfont/css/elusive-webfont.css">
	<link rel="stylesheet" href="/assets/style.css">
	<link rel="stylesheet" href="/plugins/jquery-oembed/jquery.oembed.css">
	{% block injectCSS %}
	{% endblock %}
	
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="//cdn.jsdelivr.net/html5shiv/3.7.0/html5shiv.min.js"></script>
        <script src="//cdn.jsdelivr.net/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body>
    <div class="main-wrapper">
  
      <!-- =========================================
        Menu
        ========================================== -->
		<ul id="gn-menu" class="gn-menu-main">
            <!-- trigger -->
			<li class="gn-trigger">
                <a class="gn-icon-menu" href="javascript:void(0)" title="Menu"><i class="fa fa-bars"></i></a>
				<nav class="gn-menu-wrapper">
					<div class="gn-scroller">
                        <h1 class="sr-only">Main Menu</h1>
						<ul class="gn-menu">
			        	{% include 'widgets/menu.twig' %}
			        	</ul>
					</div>

				</nav>	
			</li>

			<!-- logo -->
			<li class="logo">
                <h1><a href="{{ urlFor('home')}}" title="Nicolas Van Labeke"> <span class="base-color">N</span>VL
			</a></h1>

			<!-- page-title -->
			<li class="page-title"><h2>{% block pagetile %}{% endblock %}</h2></li>

			<!-- #menu-social-icons -->
			{% include 'widgets/social.twig' %}

		</ul>

        <article>

        <!-- =========================================
        breadcrumb
        ========================================== -->
        <!-- breadcrumb-wrapper -->
        <div class="breadcrumb-wrapper">
            <!-- container -->
		    <div class="container">
		        <!-- row -->
		        <div class="row">

                    <div class="page-description">
                    {% block breadcrumb %}{% endblock %}
					{% block pagedescription %}{% endblock %}
                    </div>

                </div><!-- /row -->
            </div><!-- /container -->
        </div><!-- /breadcrumb-wrapper -->
        
        
        {% block notification %}
        {% if flash.messages %}
        <div class="container">
		<div class="row col-sm-offset-2 col-sm-8">
			{% for key,value in flash.messages[:1] %}
        	<div class="alert alert-warning alert-dismissable">
        		<button type="button" class="close" data-dismiss="alert" 
        				 aria-hidden="true">&times;</button>
				<p><b>{{key|capitalize}}</b> - {{ flash.messages[key]|raw }}
        		
				<p></div>
			{% endfor %}
        </div></div>
        {% endif %}
		{% endblock %}

        
        <div class="content-wrapper">
        {% block content %}{% endblock %}
         <div class="push"></div>
        </div>
        </article>
        
	</div>      
    {% include 'widgets/footer.twig' %}

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
    <script src="//cdn.jsdelivr.net/i18next/1.7.1/i18next.min.js"></script>
    <script src="//cdn.jsdelivr.net/fitvids/1.1.0/jquery.fitvids.js"></script>
    <script src="//cdn.jsdelivr.net/jquery.easing/1.3/jquery.easing.1.3.js"></script>
    <script src="//cdn.jsdelivr.net/jquery.nicescroll/3.5.4/jquery.nicescroll.min.js"></script>
    <script src="//cdn.jsdelivr.net/snap.svg/0.2.0/snap.svg-min.js"></script>
    <script src="/plugins/jquery-oembed/jquery.oembed.js"></script>
    <script src="/plugins/holder.js"></script>
    <script src="/plugins/classie.js"></script>
    <script src="/plugins/gnmenu.js"></script>
    <script src="/plugins/isotope.pkgd.js"></script>

 	<!-- <script src="/plugins/gmaps.js"></script>         
    <script src="http://maps.google.com/maps/api/js?sensor=true"></script>-->
    <script src="/plugins/jNVL.js"></script>
    <script src="/plugins/Melon.js"></script>

    <script>
    $(document).ready(function() {
        $.i18n.init({
            //ns: { namespaces: ['ns.common', 'ns.special'], defaultNs: 'ns.common'},
            lng: 'dev',
            resGetPath: '/locales/__lng__/__ns__.json',
            useLocalStorage: false,
            debug: true
        }, function() {
            $("span").i18n();
        });

    });
    </script>

    {% block injectJS %}
    {% endblock %}
    <script>
	  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
	
	  ga('create', 'UA-49330125-1', 'calques3d.org');
	  ga('send', 'pageview');
	</script>
</body>
</html>
