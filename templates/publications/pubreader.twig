        }

        .sidr .lol a {
            background-color: #ddd;
            border: 1px solid transparent;
            border-radius: 4pt;
            color: #666;
            display: block;
            font-family: "HelveticaNeue-CondensedBold","Arial Narrow","ArialMT","Arial",sans-serif;
            font-size: 11pt;
            font-weight: bold;
            height: 25pt;
            line-height: 25pt;
            margin: 6pt;
            overflow: hidden;
            padding: 0 4pt;
            text-align: left;
            text-decoration: none;
            text-overflow: ellipsis;
            white-space: nowrap;
        }
        .sidr .lol a.current {
            background-color: #fea;
            font-size: 12pt;
        }
        .sidr .lol a.current, aside .lol a:hover {
            color: #333;
        }
        .sidr .lol a:hover {
            background-color: #ccc;
        }
        .sidr  .lol a:active {
            background-color: #fff;
            color: #000;
        }
