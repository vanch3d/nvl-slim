<body>
<div id="jr" data-jr-path="../">
    {% include "publications/pubreader.ui.twig" with { paper_links: block('links')} %}
<div id="jr-content">


    <article data-type="main">

        <div class="articleinfo">
                <div class="t">{{ item.output.ref|raw() }}</div>
        </div>

        <header class="fm-sec">
            <h1 class="content-title">{{ item.title }}</h1>
            <p class="contribs"> {% for auth in item.author %}
                    {{ auth.given|slice(0,1)~'.' }} {{ auth.family }} {{ loop.last ? '' : ',' }}
                {% endfor %}</p>

            <abbr class="unapi unapi-id" title="{{item.id}}"></abbr>
        </header>

        <section class="sec">
            <h2 id="__abstract">Abstract</h2>
            <p>{{ item.abstract }}</p>
        </section>

        {% block article %}{% endblock %}
    </article>
    <article id="ob-xxxsxx" class="full-details">
        {{item.output.ref|raw()}}
    </article>
    {% block figures %}{% endblock %}

</div>
