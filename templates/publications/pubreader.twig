        ol.ref-numbered {
            counter-reset: item;
            margin-left: 0;
            padding-left: 1.2em;
        }

        ol.ref-numbered li {
            list-style-type: none;
            margin-bottom: 0.5em;
            padding-left: 1.2em;
            text-indent: -1.2em;
        }

        ol.ref-numbered li:before {
            content: "[" counter(item, decimal) "]";
            counter-increment: item;
            display: inline-block;
            width: 1.2em;
        }
